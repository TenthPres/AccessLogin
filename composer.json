{
  "name": "tenth/access-login",
  "type": "library",
  "description": "Authenticate and identify church members and regular attenders by their ACS credentials.",
  "minimum-stability": "dev",
  "keywords": ["Church", "Church Membership", "Authentication"],
  "homepage": "https://github.com/TenthPres/AccessLogin",
  "license": "MIT",
  "authors": [
    {
      "name": "James Kurtz",
      "email": "jkrrv@jkrrv.org",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TenthPres/AccessLogin"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tenth\\": "src/",
      "Tenth\\AccessLogin\\": "src/AccessLogin"
    }
  },
  "archive": {
    "exclude": [
      ".idea/",
      "tests/credentials.json"
    ]
  },
  "require": {
    "guzzlehttp/guzzle": "^6.2",
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.6",
    "phpcompatibility/php-compatibility": "*",
    "squizlabs/php_codesniffer": "3.*",
    "php-coveralls/php-coveralls": "^2.2",
    "ext-json": "*"
  },
  "prefer-stable": true
}
